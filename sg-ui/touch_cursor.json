{
    "namespace": "SG_touch_cursor",
    "animation": {
        "anim_type": "flip_book",
        "easing": "linear",
        "initial_uv": [ 0, 0 ],
        "frame_count": 2,
        "frame_step": 4,
        "fps": 2
    },
    "touch_cursor": {
        "type": "image",
        "texture": "images/ui/touch_cursor",
        "size": [ 20, 20 ],
        "anchor_from": "center",
        "anchor_to": "center",
        "uv_size": [ 17, 17 ],
        "uv": "@SG_touch_cursor.animation",
        "disable_anim_fast_forward": true
    }
}
